{
    "extends": [
      "react-app",
      "airbnb",
      "plugin:jsx-a11y/recommended",
      "prettier",
      "prettier/react"
    ],
    "plugins": [
      "jsx-a11y",
      "prettier"
    ],
    "rules": {
      "semi": 0,
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "prettier/prettier": [
        "error", {
          "semi": false
        }
      ],
      "react/prop-types": 0,
      "react/sort-comp": 0,
      "react/state-in-constructor": 0,
      "import/no-cycle": 0,
      "react/destructuring-assignment": 0
    }
  }